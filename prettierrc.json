{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "gitlens.currentLine.enabled": true,
  "gitlens.statusBar.enabled": false,
  "gitlens.codeLens.enabled": true,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.inlineBlame.enabled": false,
  "gitlens.views.enabled": true,
  "gitlens.explorer.enabled": true
}